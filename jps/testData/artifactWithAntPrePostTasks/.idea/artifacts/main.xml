        <build-properties>
          <build-property name="taskName" value="pre" />
          <build-property name="myProperty" value="1" />
        </build-properties>
      </options>
    </properties>
    <root id="root">
      <element id="directory" name="dir">
        <element id="file-copy" path="$PROJECT_DIR$/file.txt" />
      </element>
    </root>
  </artifact>
</component>