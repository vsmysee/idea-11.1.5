<component name="ArtifactManager">
  <artifact name="main">
    <output-path>$PROJECT_DIR$/out/artifacts/main</output-path>
    <properties id="ant-postprocessing">
      <options enabled="true">
        <file>file://$PROJECT_DIR$/scripts/build.xml</file>
        <target>post-step</target>
        <build-properties>
        </build-properties>
      </options>
    </properties>
